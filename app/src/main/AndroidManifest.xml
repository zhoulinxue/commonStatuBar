<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhx.common.statubar.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.BlackStatusTextActivity" />
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.FragmentTestActivity" />
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.WhiteStatusTextActivity" />
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.BottomInputActivity" />
        <activity android:name="com.zhx.common.statubar.demo.org.zhx.common.statubar.demo.DialogActivity" />
    </application>

</manifest>